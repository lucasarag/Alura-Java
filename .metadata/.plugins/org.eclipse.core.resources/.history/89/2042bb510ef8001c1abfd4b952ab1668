
public class TesteIR {

	public static void main(String[] args) {
	
		double salario = 3300.0;
		
		if (salario <= 1900.0) {
			 System.out.println("Salário menor que 1900.0!");
		}else {
			if(salario <= 2800.0) {
				salario = salario - 142.0;
				System.out.println("O IR é de 7.5% e seu salário é de R$ " + salario);
			}else {
				if(salario <= 3751.0) {
					salario = salario - 350.0;
					System.out.println("O IR é de 15% e seu salário é de R$ " + salario);
				}else {
					if(salario <= 4664.0) {
						salario = salario - 636.0;
						System.out.println("O IR é de 22.5% e seu salário é de R$ " + salario);
					}else {
						System.out.println("Salário maior que 4664.0!");
					}
				}
			}				
		}
	}
}

